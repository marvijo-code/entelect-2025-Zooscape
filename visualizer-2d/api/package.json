{
  "name": "visualizer-2d-api",
  "version": "1.0.0",
  "description": "API server for Zooscape 2D Visualizer",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon --watch ./ server.js"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "nodemon": "^2.0.22"
  },
  "nodemonConfig": {
    "watch": ["./"],
    "ext": "js,json",
    "ignore": ["node_modules/*"],
    "delay": "500"
  }
}
